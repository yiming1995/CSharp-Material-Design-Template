                                    <Grid Width="75" Margin="5,0,0,0">
                                        <Rectangle RadiusX="3" RadiusY="3" Fill="#0F1525"/>
                                        <TextBlock x:Name="labGroupAction" Text="ACTION" Foreground="#dddddd" FontSize="12" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" FontFamily="Candara"/>
                                    </Grid>
                                </StackPanel>
                                <ScrollViewer VerticalScrollBarVisibility="Auto" CanContentScroll="True" Style="{StaticResource MaterialDesignScrollViewer}">
                                    <StackPanel x:Name="proxy_wrap_panel" Margin="0,5,0,0" Width="465" Height="410" CanVerticallyScroll="True" HorizontalAlignment="Center" ScrollViewer.VerticalScrollBarVisibility="Auto">
                                    </StackPanel>
                                </ScrollViewer>
                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                    <Button x:Name="clear_proxy_group_btn" Width="200" Height="30" Foreground="Black" Background="#00DEAB" Margin="20,0,0,0" Click="clear_proxy_group_btn_Click" BorderBrush="{x:Null}" BorderThickness="1">
                                        <StackPanel Orientation="Horizontal">
                                            <materialDesign:PackIcon Kind="Clear" Margin="0,2,0,0"/>
                                            <TextBlock x:Name="labClearGroup" Text="CLEAR ALL GROUP"/>
                                        </StackPanel>
                                    </Button>
                                </StackPanel>
                            </StackPanel>
                        </Grid>
                        <Grid>
                            <Rectangle Fill="#1E263D" RadiusX="5" RadiusY="5" Margin="20,20,20,-20" Width="560"/>
                            <StackPanel Orientation="Vertical" HorizontalAlignment="Center" Margin="0,30,0,0">
                                <TextBlock x:Name="labProxyList" Text="PROXY LIST" FontSize="13" Foreground="#dddddd" FontWeight="Bold"/>
                                <StackPanel Orientation="Horizontal" Height="25" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,10,0,0">
                                    <Grid Width="50">
                                        <Rectangle RadiusX="3" RadiusY="3" Fill="#0F1525"/>
                                        <TextBlock Grid.Column="0" Text="NO" Foreground="#dddddd"  FontSize="12" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" FontFamily="Candara"/>
                                    </Grid>
                                    <Grid Width="250" Margin="5,0,0,0">
                                        <Rectangle RadiusX="3" RadiusY="3" Fill="#0F1525"/>
                                        <TextBlock x:Name="labEditProxy"  Grid.Column="1" Text="PROXY" Foreground="#dddddd"  FontSize="12" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" FontFamily="Candara"/>
                                    </Grid>
                                    <Grid Width="100" Margin="5,0,0,0">
                                        <Rectangle RadiusX="3" RadiusY="3" Fill="#0F1525"/>
                                        <TextBlock x:Name="labEditProxyStatus" Grid.Column="2" Text="STATUS" Foreground="#dddddd"  FontSize="12" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" FontFamily="Candara"/>
                                    </Grid>
                                    <Grid Width="50" Margin="5,0,0,0">
                                        <Rectangle RadiusX="3" RadiusY="3" Fill="#0F1525"/>
                                        <TextBlock x:Name="labEditProxyAction" Grid.Column="3" Text="ACTION" Foreground="#dddddd"  FontSize="12" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" FontFamily="Candara"/>
                                    </Grid>
                                </StackPanel>
                                <ScrollViewer VerticalScrollBarVisibility="Auto" CanContentScroll="True" Style="{StaticResource MaterialDesignScrollViewer}">
                                    <StackPanel x:Name="Edit_Proxy_Group_panel" Width="470" Height="310" CanVerticallyScroll="True" ScrollViewer.VerticalScrollBarVisibility="Auto" Margin="5,5,0,0">
                                    </StackPanel>
                                </ScrollViewer>

                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Margin="0,30,0,0">
                                    <TextBox x:Name="proxy_group_name_input" Style="{StaticResource MaterialDesignOutlinedTextFieldTextBox}" materialDesign:HintAssist.Hint="Proxy Group Name" Foreground="White" FontSize="14" Width="150" materialDesign:HintAssist.Background="#0F1525" />
                                    <Grid Margin="30,0,0,0">
                                        <Rectangle Fill="#0F1525" RadiusX="4" RadiusY="4" Width="150"/>
                                        <StackPanel Orientation="Horizontal"  HorizontalAlignment="Center">
                                            <TextBlock x:Name="lab_proxycount" Text="Proxy Count :" Foreground="#00dd00" VerticalAlignment="Center" FontSize="14"/>
                                            <TextBlock x:Name="proxy_count_show" Foreground="White" VerticalAlignment="Center" FontSize="14" Width="20" Margin="5,0,0,0"/>
                                        </StackPanel>
                                    </Grid>
                                </StackPanel>

                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Margin="0,20,0,0">
                                    <Button x:Name="testproxy_btn" FontSize="13" Width="100" Foreground="Black" Height="30" Background="#00DEAB" Click="testproxy_btn_Click" BorderBrush="{x:Null}">
                                        <StackPanel Orientation="Horizontal">
                                            <materialDesign:PackIcon Kind="CheckNetwork"  Width="12" Height="12" Margin="0,3,0,0"/>
                                            <TextBlock x:Name="labProxyTest" Text="Test" Margin="4,0,0,0"/>
                                        </StackPanel>
                                    </Button>
                                    <Button x:Name="addproxy_btn" FontSize="13" Width="100" Margin="10,0,0,0" Foreground="Black" Height="30" Background="#00DEAB" Click="addproxy_btn_Click" BorderBrush="{x:Null}">
                                        <StackPanel Orientation="Horizontal">
                                            <materialDesign:PackIcon Kind="Add" Width="12" Height="12" Margin="0,3,0,0"/>
                                            <TextBlock x:Name="labProxyAdd" Text="Add" Margin="4,0,0,0"/>
                                        </StackPanel>
                                    </Button>
                                    <Button x:Name="clearproxy_btn" FontSize="13" Width="100" Margin="10,0,0,0" Height="30" Background="#00DEAB" Click="clearproxy_btn_Click" BorderBrush="White" Foreground="Black">
                                        <StackPanel Orientation="Horizontal" VerticalAlignment="Center">
                                            <materialDesign:PackIcon Kind="Clear" Width="12" Height="12" Margin="0,3,0,0"/>
                                            <TextBlock x:Name="labProxyClear" Text="Clear" Margin="4,0,0,0"/>
                                        </StackPanel>
                                    </Button>
                                    <Button x:Name="saveproxy_btn" FontSize="13" Width="100" Margin="10,0,0,0" Height="30" Background="#00DEAB" Click="saveproxy_btn_Click" BorderBrush="{x:Null}" Foreground="Black">
                                        <StackPanel Orientation="Horizontal">
                                            <materialDesign:PackIcon Kind="ContentSaveAll" Width="12" Height="12" Margin="0,4,0,0"/>
                                            <TextBlock x:Name="labProxySave" Text="Save" Margin="4,0,0,0"/>
                                        </StackPanel>
                                    </Button>
                                </StackPanel>
                            </StackPanel>
                        </Grid>
                    </StackPanel>
                </StackPanel>
            </Grid>
        </StackPanel>
        <StackPanel x:Name="panSetting" Margin="0,50,0,0" Visibility="Hidden" >
            <Grid Width="1200" Height="640">
                <Rectangle Fill="#1B2A49" RadiusX="5" RadiusY="5" Stroke="#121223" StrokeThickness="1"/>
                <StackPanel Orientation="Horizontal">
                    <StackPanel Orientation="Vertical">
                        <Grid Width="1200" Height="40">
                            <Rectangle Fill="#0F1525"/>
                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                <materialDesign:PackIcon Kind="SettingsOutline"  Foreground="#dddddddd" VerticalAlignment="Center" Margin="0,0,3,0"/>
                                <TextBlock x:Name="labSettingPanel" Text="Settings Panel" Foreground="#dddddddd" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="18" FontFamily="Candara"/>
                            </StackPanel>
                        </Grid>
                        <StackPanel Orientation="Horizontal" Margin="30,30,0,0">
                            <StackPanel Orientation="Vertical">
                                <GroupBox x:Name="groupwebhook" Header="WebHook" FontWeight="Bold" FontSize="14" >
                                    <StackPanel Orientation="Vertical">
                                        <TextBox x:Name="txtwebhook" Width="300" Height="160" Foreground="#dddddd" TextChanged="txtwebhook_TextChanged" TextWrapping="Wrap"/>
                                        <Button x:Name="btnTestWebhook" Width="200" Margin="0,10,0,0" Click="btnTestWebhook_Click" Background="#00DEAB" Foreground="Black" BorderBrush="{x:Null}">
                                            <StackPanel Orientation="Horizontal">
                                                <materialDesign:PackIcon Kind="Webhook" Margin="0,2,3,0"/>
                                                <TextBlock x:Name="txtTestWebhook" Text="Test Webhook"/>
                                            </StackPanel>
                                        </Button>
                                    </StackPanel>
                                </GroupBox>
                                <GroupBox x:Name="groupSoundSetting" Header="Sound Setting" Margin="0,40,0,0" Width="400"  FontWeight="Bold" FontSize="14">
                                    <StackPanel Orientation="Vertical">
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                            <CheckBox x:Name="chkbackmusic" VerticalAlignment="Bottom" Height="30" Width="30" Click="chkbackmusic_Click"/>
                                            <ComboBox x:Name="cmbBackGroundSound" Margin="0,10,0,0"  Foreground="#dddddd" materialDesign:HintAssist.IsFloating="True" Width="250" SelectionChanged="cmbBackGroundSound_SelectionChanged">
                                                <materialDesign:HintAssist.Hint>
                                                    <TextBlock Text="BackGround Music"/>
                                                </materialDesign:HintAssist.Hint>
                                            </ComboBox>
                                        </StackPanel>
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                            <CheckBox x:Name="chksucsound" VerticalAlignment="Bottom" Height="30" Width="30" Click="chksucsound_Click"/>
                                            <ComboBox x:Name="cmbSuccessSound" Margin="0,10,0,0" Foreground="#dddddd" materialDesign:HintAssist.IsFloating="True" Width="250" SelectionChanged="cmbSuccessSound_SelectionChanged">
                                                <materialDesign:HintAssist.Hint>
                                                    <TextBlock Text="Success Sound"/>
                                                </materialDesign:HintAssist.Hint>
                                            </ComboBox>
                                        </StackPanel>
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Margin="0,0,0,30">
                                            <CheckBox x:Name="chkwarnsnd" VerticalAlignment="Bottom" Height="30" Width="30" Click="chkwarnsnd_Click"/>
                                            <ComboBox x:Name="cmbWarningSound" Margin="0,10,0,0"  Foreground="#dddddd" materialDesign:HintAssist.IsFloating="True" Width="250" SelectionChanged="cmbWarningSound_SelectionChanged">
                                                <materialDesign:HintAssist.Hint>
                                                    <TextBlock Text="Warning Sound"/>
                                                </materialDesign:HintAssist.Hint>
                                            </ComboBox>
                                        </StackPanel>
                                    </StackPanel>
                                </GroupBox>
                            </StackPanel>
                        </StackPanel>
                    </StackPanel>
                </StackPanel>
            </Grid>
        </StackPanel>
        <StackPanel x:Name="FootPanel" Orientation="Vertical" VerticalAlignment="Bottom" Margin="0,0,0,10" >
            <Line Width="800" Fill="White" Stroke="White" Height="5" X2="800" Y2="0" StrokeThickness="1"/>
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Margin="0,5,0,0">
                <TextBlock x:Name="labRefreshTime" Width="80" Text="Refresh Time:" Foreground="White" VerticalAlignment="Center" />
                <TextBox x:Name="txtRetryInterval" Width="60" Height="40" Style="{StaticResource MaterialDesignOutlinedTextFieldTextBox}" Margin="5,0,0,0" Foreground="White" Padding="0,0,0,0" Text="500" FontSize="12" TextChanged="txtRetryInterval_TextChanged"/>
                <TextBlock x:Name="labRetryingLimit" Width="90" Text="Retrying Limits:" Foreground="White"  VerticalAlignment="Center" Margin="20,0,0,0"/>
                <TextBox x:Name="txtRetryCount" Width="60" Height="40" Style="{StaticResource MaterialDesignOutlinedTextFieldTextBox}" Margin="5,0,0,0" Foreground="White" Padding="0,0,0,0" Text="5" FontSize="12" TextChanged="txtRetryCount_TextChanged"/>
                <TextBlock x:Name="labSleepTime" Width="70" Text="Sleep time:" Foreground="White" VerticalAlignment="Center" Margin="20,0,0,0"/>
                <TextBox x:Name="txtOrderSleepTime" Width="60" Height="40" Style="{StaticResource MaterialDesignOutlinedTextFieldTextBox}" Margin="5,0,0,0" Foreground="White" Padding="0,0,0,0" Text="300" FontSize="12" TextChanged="txtOrderSleepTime_TextChanged"/>
                <TextBlock x:Name="labGA" Width="70" Text="GA Token:" Foreground="White" VerticalAlignment="Center" Margin="20,0,0,0"/>
                <TextBox x:Name="txtGA" Width="360" Height="40" Style="{StaticResource MaterialDesignOutlinedTextFieldTextBox}" Margin="5,0,0,0" Foreground="White" Padding="0,0,0,0" FontSize="12" TextChanged="txtGA_TextChanged" />
            </StackPanel>
        </StackPanel>
        <local:VolumeControl x:Name="cntVolume" VerticalAlignment="Top" HorizontalAlignment="Right" Margin="0,40,10,0" Visibility="Hidden"/>
    </Grid>
</Window>
